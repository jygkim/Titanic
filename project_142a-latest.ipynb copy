   "source": [
    "parameter = fit(x, y,0.02,500)\n",
    "expression = np.dot(x_test, parameter[0]) + parameter[1]\n",
    "predict = sigmoid(expression) >= 1/2\n",
    "result = []\n",
    "for i in predict:\n",
    "    if i == True:\n",
    "        result.append(1)\n",
    "    else:\n",
    "        result.append(0)\n",
    "result"
   ]
