   "outputs": [],
   "source": [
    "# convert age into groups\n",
    "def conv_age(data):\n",
    "    res = []\n",
    "    for i in data:\n",
    "        if i <= 20:\n",
    "            res.append(0)\n",
    "        elif i <= 40:\n",
    "            res.append(1)\n",
    "        elif i <= 65:\n",
    "            res.append(2)\n",
    "        else:\n",
    "            res.append(3)\n",
    "    return res     "
   ]
  },
